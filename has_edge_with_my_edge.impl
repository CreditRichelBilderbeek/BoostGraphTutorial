#include <boost/graph/properties.hpp>
#include "install_edge_custom_type.h"
#include "my_edge.h"

template <typename graph>
bool has_edge_with_my_edge(
  const my_edge& e,
  const graph& g
) noexcept
{
  const auto my_edges_map = get(boost::edge_custom_type,g);

  for (auto p = edges(g);
    p.first != p.second;
    ++p.first) {
    if (get(my_edges_map, *p.first) == e) {
      return true;
    }
  }
  return false;
}
