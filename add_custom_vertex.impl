#include <boost/graph/adjacency_list.hpp>
#include "install_vertex_custom_type.h"
#include "my_vertex.h"

template <typename graph>
void add_custom_vertex(graph& g, const my_vertex& v)
{
  const auto vertex_descriptor_a = boost::add_vertex(g);
  const auto my_vertex_map = boost::get(boost::vertex_custom_type,g);
  my_vertex_map[vertex_descriptor_a] = v;
}
