#include <cassert>
#include "add_named_vertex.h"
#include "count_vertices_with_name.h"
#include "create_empty_undirected_named_vertices_graph.h"
#include "create_named_vertices_path_graph.h"

void count_vertices_with_name_demo() noexcept
{
  auto g = create_named_vertices_path_graph(
    {"Apple", "Pear", "Apple"}
  );
  assert(count_vertices_with_name("Apple", g) == 2);
  assert(count_vertices_with_name("Pear", g) == 1);
  assert(count_vertices_with_name("Banana", g) == 0);
}
