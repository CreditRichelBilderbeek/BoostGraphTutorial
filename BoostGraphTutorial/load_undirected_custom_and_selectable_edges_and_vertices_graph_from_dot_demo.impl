#include <cassert>
#include "create_custom_and_selectable_edges_and_vertices_k2_graph.h"
#include "is_regular_file.h"
#include "save_custom_and_selectable_edges_and_vertices_graph_to_dot.h"

void load_undirected_custom_and_selectable_edges_and_vertices_graph_from_dot_demo() noexcept
{
  const auto g
    = create_custom_and_selectable_edges_and_vertices_k2_graph();
  const std::string filename{
    "create_custom_and_selectable_edges_and_vertices_k2_graph.dot"
  };
  save_custom_and_selectable_edges_and_vertices_graph_to_dot(
    g,
    filename
  );
  assert(is_regular_file(filename));
}
