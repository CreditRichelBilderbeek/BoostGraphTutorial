#include <cassert>
#include "create_custom_vertices_path_graph.h"

void create_custom_vertices_path_graph_demo() noexcept
{
  const auto g = create_custom_vertices_path_graph(
    {
      my_custom_vertex("A"),
      my_custom_vertex("B"),
      my_custom_vertex("C")
    }
  );
  assert(boost::num_edges(g) == 2);
  assert(boost::num_vertices(g) == 3);
}
